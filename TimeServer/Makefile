TIMESERVER_ITERATIVE = timeserver_iterative
TIMESERVER_ITERATIVE_OBJS = $(addsuffix .o, $(TIMESERVER_ITERATIVE))

PROGS = $(TIMESERVER_ITERATIVE)
OBJS = $(TIMESERVER_ITERATIVE_OBJS)

include ../Make.bin

$(TIMESERVER_ITERATIVE): $(TIMESERVER_ITERATIVE_OBJS)
	$(CC) $(CFLAGS) -L$(LIBCHELPER_LIB) -L$(LIBCNET_LIB) -o $@ $? -l$(LIBCHELPER) -l$(LIBCNET)
